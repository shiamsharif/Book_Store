<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title> {% block title %}{% endblock title %} </title>
  <!-- Bootstrap Css -->
  {% load static %}
  <link rel="stylesheet" href="{% static 'css/bootstrap.min.css' %}">
  <!-- Custom Css -->
  <link rel="stylesheet" href="{% static 'custom_css/style.css' %}">
  <!-- Icon -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css">
  <!-- FONT -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,opsz,wght@0,6..12,200..1000;1,6..12,200..1000&family=Space+Grotesk&display=swap" rel="stylesheet">

</head>

<body>

  <div class="">
    {% block content %}
    {% endblock content %}
  </div>

  <script src="{% static 'js/bootstrap.bundle.min.js' %}"></script>
</body>

</html>